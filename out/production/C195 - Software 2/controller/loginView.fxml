<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="400" maxWidth="500" minHeight="400" minWidth="500" prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginController">
   <children>
      <Button fx:id="loginButton" layoutX="224.0" layoutY="215.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="25.0" prefWidth="149.0" text="Login">
         <font>
            <Font name="System Bold" size="11.0" />
         </font></Button>
      <TextField fx:id="passwordField" layoutX="224.0" layoutY="174.0" />
      <TextField fx:id="usernameField" layoutX="224.0" layoutY="134.0" />
      <Text fx:id="usernameText" layoutX="81.0" layoutY="155.0" strokeType="OUTSIDE" strokeWidth="3.0" text="Username">
         <font>
            <Font name="Tw Cen MT" size="27.0" />
         </font></Text>
      <Text fx:id="passwordText" layoutX="83.0" layoutY="194.0" strokeType="OUTSIDE" strokeWidth="3.0" text="Password">
         <font>
            <Font name="Tw Cen MT" size="27.0" />
         </font>
      </Text>
      <Text fx:id="locationLabel" layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Location: " />
      <Text fx:id="localeLabel" layoutX="19.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LOCALE" />
      <Button fx:id="cancelButton" layoutX="224.0" layoutY="253.0" mnemonicParsing="false" onAction="#onCancelClick" prefHeight="25.0" prefWidth="149.0" text="Cancel">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
