<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.customerController">
   <children>
      <TextField fx:id="customerIdField" editable="false" layoutX="101.0" layoutY="78.0" />
      <Text layoutX="14.0" layoutY="96.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer ID" />
      <TextField fx:id="customerNameField" layoutX="101.0" layoutY="103.0" />
