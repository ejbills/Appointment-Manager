      <Text layoutX="15.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Postal Code" />
      <Text layoutX="31.0" layoutY="243.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone" />
